# N8N Configuration
N8N_HOST=n8n.yourdomain.com
N8N_PORT=5678
N8N_PROTOCOL=https
N8N_ENCRYPTION_KEY=<CHANGE_THIS_TO_A_RANDOM_32_CHAR_STRING>
N8N_USER_MANAGEMENT_DISABLED=false
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=<YOUR_ADMIN_USER>
N8N_BASIC_AUTH_PASSWORD=<YOUR_STRONG_PASSWORD>

# PostgreSQL Configuration
POSTGRES_USER=n8n
POSTGRES_PASSWORD=<YOUR_STRONG_POSTGRES_PASSWORD>
POSTGRES_DB=n8n
POSTGRES_NON_ROOT_PASSWORD=<YOUR_STRONG_POSTGRES_PASSWORD>
DB_POSTGRESDB_DATABASE=n8n
DB_POSTGRESDB_SCHEMA=public
DB_TYPE=postgresdb

# Redis Configuration (for queue mode)
REDIS_HOST=redis
REDIS_PASSWORD=<YOUR_STRONG_REDIS_PASSWORD>

# Queue Mode Configuration
N8N_QUEUE_BULL_REDIS_HOST=redis
N8N_QUEUE_BULL_REDIS_PASSWORD=<YOUR_STRONG_REDIS_PASSWORD>

# Execution Configuration
EXECUTIONS_DATA_SAVE_ON_ERROR=all
EXECUTIONS_DATA_SAVE_ON_SUCCESS=all
EXECUTIONS_DATA_SAVE_ON_PROGRESS=true
EXECUTIONS_DATA_SAVE_MANUAL_EXECUTIONS=true

# Security Configuration
N8N_HIRING_BANNER_ENABLED=false
N8N_PERSONALIZATION_ENABLED=false
N8N_DIAGNOSTICS_ENABLED=false
N8N_SECURITY_BLOCK_CLI_WEBHOOK=true

# Performance Configuration
N8N_METRICS=true

# SSL Configuration
DOMAIN=yourdomain.com
SSL_EMAIL=your-email@domain.com

# Backup Configuration
BACKUP_RETENTION_DAYS=7
BACKUP_DIR=/path/to/backup/directory

# Monitoring Configuration
SLACK_WEBHOOK_URL=<YOUR_SLACK_WEBHOOK_URL>
DISCORD_WEBHOOK_URL=<YOUR_DISCORD_WEBHOOK_URL> 